{
  "name": "r-anwar/craft-glossary",
  "description": "Add glossaries with tooltips to Craft CMS.",
  "version": "2.0.1",
  "type": "craft-plugin",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "glossary"
  ],
  "license": "proprietary",
  "authors": [
    {
      "name": "codemonauts",
      "homepage": "https://codemonauts.com"
    }
  ],
  "support": {
    "source": "https://github.com/r-anwar/craft-glossary",
    "docs": "https://plugins.codemonauts.com/plugins/glossary/Introduction.html",
    "issues": "https://github.com/r-anwar/craft-glossary/issues"
  },
  "require": {
    "craftcms/cms": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "codemonauts\\glossary\\": "src/"
    }
  },
  "extra": {
    "handle": "glossary",
    "class": "codemonauts\\glossary\\Glossary",
    "name": "Glossary"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
